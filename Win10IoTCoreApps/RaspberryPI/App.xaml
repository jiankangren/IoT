<Application
    x:Class="HomeSensorApp.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:services="using:HomeSensorApp.Services"
    RequestedTheme="Dark">

    <Application.Resources>
        <services:SettingsService x:Key="ApplicationSettings" />
        <services:SensorService x:Key="SensorService" />
        <services:IotHubService x:Key="IoTHubService" />
    </Application.Resources>
    
</Application>
